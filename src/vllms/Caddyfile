:8080 {
    handle /qwen/* {
        uri strip_prefix /qwen
        reverse_proxy 127.0.0.1:{env.QWEN_PORT}
    }
    handle /mistral/* {
        uri strip_prefix /mistral
        reverse_proxy 127.0.0.1:{env.MISTRAL_PORT}
    }
    handle /deepseek/* {
        uri strip_prefix /deepseek
        reverse_proxy 127.0.0.1:{env.DEEPSEEK_PORT}
    }

    respond /health "ok" 200
}